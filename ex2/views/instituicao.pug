extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Contratos Instituição
    
    .w3-container
      table.w3-table-all
        tr
          th Id
          th Inicio de Contrato
          th Data de Fim de Contrato
          th Nome da Instituicao
          th Areas de Investigacao
        
        each s in clist
          tr
            td
              a(href="/contracts/"+s.id)= s.id
            td=s.DataInicioContrato
            td=s.DataFimContrato
            td=s.NomeInstituicao
            td=s.AreasInvestigacao

    footer.w3-container.w3-teal 
      h5 Generated by RPCW2023 in #{d} - #[a(href="/") Return]


